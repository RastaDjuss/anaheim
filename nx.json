{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "anchor-build": {
      "outputs": [
        "{projectRoot}/build"
      ],
      "cache": true
    },
    "anchor-test": {
      "outputs": [
        "{projectRoot}/coverage"
      ],
      "cache": true
    },
    "dev": {
      "outputs": [
        "{projectRoot}/build"
      ],
      "cache": true
    },
    "build": {
      "outputs": [
        "{projectRoot}/buiold"
      ],
      "cache": true
    },
    "anchor": {
      "cache": true
    },
    "anchor-localnet": {
      "cache": true
    },
    "ci": {
      "cache": true
    },
    "start": {
      "cache": true
    },
    "lint": {
      "cache": true
    }
  },
  "defaultBase": "master",
  "plugins": [
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "eslint:lint"
      }
    },
    {
      "plugin": "@nx/jest/plugin",
      "options": {
        "targetName": "test"
      }
    },
    {
      "plugin": "@nx/next/plugin",
      "options": {
        "startTargetName": "next:start",
        "buildTargetName": "next:build",
        "devTargetName": "next:dev",
        "serveStaticTargetName": "serve-static",
        "buildDepsTargetName": "build-deps",
        "watchDepsTargetName": "watch-deps"
      }
    }
  ],
  "nxCloudId": "67da1f4d39b4cf1988ad3228"
}
